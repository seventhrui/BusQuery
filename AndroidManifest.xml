<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.seventh.busquery" 
	android:versionCode="1" 
	android:versionName="1.0">
	<uses-sdk android:minSdkVersion="8" />

	<application
	    android:icon="@drawable/ic_launcher" 
	    android:label="@string/app_name">
		<activity 
		    android:name="com.seventh.busquery.Main_Activity" 
		    android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="com.seventh.busquery.tab.Change_Query" />
		<activity android:name="com.seventh.busquery.tab.Stop_Query" />
		<activity android:name="com.seventh.busquery.tab.Route_Query" />
		
		<activity android:name="com.seventh.busquery.result.StopResult" />
		<activity android:name="com.seventh.busquery.result.RouteResult" />
	</application>
</manifest>